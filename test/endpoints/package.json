{
  "name": "lux-endpoint-tester",
  "version": "1.0.0",
  "type": "module",
  "description": "Automated endpoint testing for LUX MarkLogic API",
  "main": "run-tests.js",
  "scripts": {
    "test": "node run-tests.js ./configs",
    "test:dev": "node run-tests.js ./configs ./reports",
    "test:save": "node run-tests.js ./configs ./reports --save-responses",
    "create:tests": "node create-tests.js",
    "create:spec": "node create-endpoints-spec.js",
    "compare": "node compare-reports.js",
    "install-deps": "npm install"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "xlsx": "^0.18.5",
    "csv-parser": "^3.0.0",
    "commander": "^12.0.0",
    "chart.js": "^4.5.1"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "author": "LUX Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/project-lux/lux-marklogic.git"
  }
}
